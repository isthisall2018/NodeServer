<service>
	<id>node_server.exe</id>
	<name>node_server</name>
	<description>测试项目服务器</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\Administrator\Desktop\demo\myapp\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Users\Administrator\Desktop\demo\myapp\main</argument>
	<argument>--log</argument>
	<argument>node_server wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>C:\Users\Administrator\Desktop\demo\myapp\config</workingdirectory>
</service>